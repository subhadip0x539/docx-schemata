{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "docIds": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "processTag": {
      "type": "string",
      "enum": ["PDFMERGE"]
    },
    "config": {
      "type": "object",
      "properties": {
        "docOrder": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["docOrder"]
    }
  },
  "required": ["docIds", "processTag", "config"]
}
